{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "import/no-cycle": "off",
    "no-restricted-exports": "off",
    "eol-last": "off",
    "react/jsx-filename-extension": "off",
    "react/no-array-index-key":"off"
  }
}
